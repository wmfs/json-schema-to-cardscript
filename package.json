{
  "name": "@wmfs/json-schema-to-cardscript",
  "description": "Scaffold Cardscript from a JSON-Schema schema",
  "version": "0.0.0-semantically-released",
  "author": "West Midlands Fire Service",
  "keywords": [
    "tymly",
    "cardscript"
  ],
  "main": "./lib/index.js",
  "dependencies": {
    "@wmfs/tymly-data-types": "1.5.0",
    "lodash": "4.17.14",
    "lodash-inflection": "1.5.0"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "codecov": "3.1.0",
    "conventional-changelog-metahub": "2.0.2",
    "cz-conventional-changelog": "2.1.0",
    "mocha": "6.1.4",
    "nyc": "14.1.1",
    "semantic-release": "15.13.19",
    "standard": "12.0.1",
    "@semantic-release/changelog": "3.0.2",
    "@semantic-release/git": "7.0.16"
  },
  "scripts": {
    "lint": "standard",
    "test": "nyc mocha && standard",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "semantic-release": "semantic-release"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wmfs/json-schema-to-cardscript.git"
  },
  "bugs": {
    "url": "https://github.com/wmfs/json-schema-to-cardscript/issues"
