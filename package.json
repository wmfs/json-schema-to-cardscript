{
  "name": "@wmfs/json-schema-to-cardscript",
  "description": "Scaffold Cardscript from a JSON-Schema schema",
  "version": "0.0.0-semantically-released",
  "author": "West Midlands Fire Service",
  "keywords": [
    "tymly",
    "cardscript"
  ],
  "main": "./lib/index.js",
  "dependencies": {
    "@wmfs/tymly-data-types": "1.5.0",
    "lodash": "4.17.21",
    "lodash-inflection": "1.5.0"
  },
  "devDependencies": {
    "chai": "4.5.0",
    "codecov": "3.8.3",
    "conventional-changelog-conventionalcommits": "8.0.0",
    "cz-conventional-changelog": "3.3.0",
    "mocha": "11.3.0",
    "nyc": "17.1.0",
    "semantic-release": "24.2.3",
    "standard": "17.1.2",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/release-notes-generator": "14.0.3",
    "@semantic-release/git": "10.0.1"
  },
  "scripts": {
    "lint": "standard",
    "test": "nyc mocha",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "semantic-release": "semantic-release"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wmfs/json-schema-to-cardscript.git"
  },
  "bugs": {
    "url": "https://github.com/wmfs/json-schema-to-cardscript/issues"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  }
